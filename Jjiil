scrollFrame.Parent = chatFrame

local outputList = Instance.new("UIListLayout")
outputList.SortOrder = Enum.SortOrder.LayoutOrder
outputList.Padding = UDim.new(0,5)
outputList.Parent = scrollFrame

-- =========================
-- Memory
-- =========================
local sessionMemory = {}
local MAX_MEMORY = 15
local function addToMemory(role, content)
    table.insert(sessionMemory, {role=role, content=content})
    if #sessionMemory > MAX_MEMORY then table.remove(sessionMemory, 1) end
end

-- =========================
-- Auto Scroll
-- =========================
local function updateChatScroll()
    scrollFrame.CanvasSize = UDim2.new(0,0,0,outputList.AbsoluteContentSize.Y + 10)
    scrollFrame.CanvasPosition = Vector2.new(0, scrollFrame.CanvasSize.Y.Offset)
end
outputList:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(updateChatScroll)

-- =========================
-- Create Messages
-- =========================
local function createAIMessage(text)
    local msgLabel = Instance.new("TextButton")
    msgLabel.Size = UDim2.new(1,-20,0,20)
    msgLabel.BackgroundTransparency = 1
    msgLabel.Text = text
    msgLabel.TextColor3 = Color3.fromRGB(100,255,100)
    msgLabel.Font = Enum.Font.GothamSemibold
    msgLabel.TextSize = 14
    msgLabel.TextWrapped = true
    msgLabel.TextXAlignment = Enum.TextXAlignment.Left
    msgLabel.AutomaticSize = Enum.AutomaticSize.Y
    msgLabel.Parent = scrollFrame
    msgLabel.MouseButton1Click:Connect(function() copyToClipboard(text) end)
    addToMemory("assistant", text)
    updateChatScroll()
end

local function createUserMessage(text)
    local msgLabel = Instance.new("TextLabel")
    msgLabel.Size = UDim2.new(1,-20,0,20)
    msgLabel.BackgroundTransparency = 1
    msgLabel.Text = text
    msgLabel.TextColor3 = Color3.fromRGB(255,255,255)
    msgLabel.Font = Enum.Font.GothamSemibold
    msgLabel.TextSize = 14
    msgLabel.TextWrapped = true
    msgLabel.TextXAlignment = Enum.TextXAlignment.Left
    msgLabel.AutomaticSize = Enum.AutomaticSize.Y
    msgLabel.Parent = scrollFrame
    addToMemory("user", text)
    updateChatScroll()
end

-- =========================
-- Send Message + API + /clear
-- =========================
local function sendMessage(message)
    if message == "/clear" then
        for _,v in pairs(scrollFrame:GetChildren()) do
            if v:IsA("TextLabel") or v:IsA("TextButton") then v:Destroy() end
        end
        sessionMemory = {}
        return
    end

    createUserMessage(message)
    createAIMessage("‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏û‡∏¥‡∏°‡∏û‡πå...")

    local messagesForAPI = {{role="system", content=system_prompt}}
    for _,v in ipairs(sessionMemory) do table.insert(messagesForAPI, v) end

    spawn(function()
        local success,response = pcall(function()
            return game:GetService("HttpService"):RequestAsync({
                Url = config.base_url.."/chat/completions",
                Method = "POST",
                Headers = {["Content-Type"]="application/json",["Authorization"]="Bearer "..config.api_key},
                Body = game:GetService("HttpService"):JSONEncode({model=config.model,messages=messagesForAPI})
            })
        end)
        if success and response.Success and response.StatusCode==200 then
            local data = game:GetService("HttpService"):JSONDecode(response.Body)
            if data and data.choices and #data.choices>0 then
                createAIMessage(data.choices[1].message.content)
            else
                createAIMessage("‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡∏õ‡∏£‡∏∞‡∏°‡∏ß‡∏•‡∏ú‡∏•")
            end
        else
            createAIMessage("‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠ API")
        end
    end)
end

sendBtn.MouseButton1Click:Connect(function()
    if inputBox.Text~="" then
        sendMessage(inputBox.Text)
        inputBox.Text=""
    end
end)

inputBox.FocusLost:Connect(function(enterPressed)
    if enterPressed and inputBox.Text~="" then
        sendMessage(inputBox.Text)
        inputBox.Text=""
    end
end)

-- =========================
-- Script Tab + Buttons
-- =========================
local scriptScroll = Instance.new("ScrollingFrame")
scriptScroll.Size = UDim2.new(1,0,1,0)
scriptScroll.Position = UDim2.new(0,0,0,0)
scriptScroll.BackgroundColor3 = Color3.fromRGB(20,20,20)
scriptScroll.BorderSizePixel = 0
scriptScroll.ScrollBarThickness = 6
scriptScroll.ScrollBarImageColor3 = Color3.fromRGB(80,80,80)
scriptScroll.Parent = scriptFrame
local scriptScrollCorner = Instance.new("UICorner")
scriptScrollCorner.CornerRadius = UDim.new(0,12)
scriptScrollCorner.Parent = scriptScroll

local gridLayout = Instance.new("UIGridLayout")
gridLayout.CellSize = UDim2.new(0,90,0,30)
gridLayout.CellPadding = UDim2.new(0,10,0,10)
gridLayout.FillDirectionMaxCells = 3
gridLayout.SortOrder = Enum.SortOrder.LayoutOrder
gridLayout.Parent = scriptScroll

local function updateScriptScroll()
    scriptScroll.CanvasSize = UDim2.new(0,0,0,gridLayout.AbsoluteContentSize.Y + 10)
end
gridLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(updateScriptScroll)

local function createScriptButton(name,color,url)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0,90,0,30)
    btn.BackgroundColor3 = color
    btn.BorderSizePixel = 0
    btn.Text = name
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.Font = Enum.Font.GothamSemibold
    btn.TextSize = 14
    btn.Parent = scriptScroll
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0,10)
    corner.Parent = btn
    btn.MouseButton1Click:Connect(function()
        pcall(function() loadstring(game:HttpGet(url))() end)
    end)
end

-- Example Script Buttons
