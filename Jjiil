createScriptButton("F3X", Color3.fromRGB(90,90,90), "https://raw.githubusercontent.com/infyiff/backup/main/F3X.lua")
createScriptButton("‡∏õ‡∏∏‡πà‡∏°4", Color3.fromRGB(120,60,60), "https://example.com/script4.lua")
